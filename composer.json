{
    "name": "sadmachine/craft-twofactorauthentication",
    "description": "Craft 3 plugin for two-factor or two-step login using Time Based OTP.",
    "type": "craft-plugin",
    "keywords": [
        "craftcms-plugin",
        "two-factor authentication",
        "two-factor",
        "two-step",
        "authentication",
        "login",
        "craftcms"
    ],
    "license": "MIT",
    "homepage": "https://github.com/sadmachine/craft-twofactorauthentication",
    "authors": [
        {
            "name": "Roel van Hintum",
            "email": "roel@born05.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "craftcms/cms": "^3.1",
        "spomky-labs/otphp": "^10.0.1",
        "endroid/qr-code": "4.0.2"
    },
    "autoload": {
        "psr-4": {
            "born05\\twofactorauthentication\\": "src/"
        }
    },
    "support": {
        "email": "austinfishbaugh@imarc.com",
        "issues": "https://github.com/sadmachine/craft-twofactorauthentication/issues",
        "source": "https://github.com/sadmachine/craft-twofactorauthentication",
        "docs": "https://github.com/sadmachine/craft-twofactorauthentication/blob/craft-3.1/README.md",
        "rss": "https://github.com/sadmachine/craft-twofactorauthentication/releases.atom"
    },
    "extra": {
        "name": "Two-Factor Authentication",
        "handle": "two-factor-authentication",
        "developer": "Born05",
        "developerUrl": "https://born05.com",
        "documentationUrl": "https://github.com/sadmachine/craft-twofactorauthentication",
        "changelogUrl": "https://raw.githubusercontent.com/sadmachine/craft-twofactorauthentication/craft-3.1/CHANGELOG.md"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
